{
	"自定义-1行省略": {
		"prefix": "ellipsis1",
		"body": [
			"overflow: hidden;",
			"text-overflow: ellipsis;",
			"white-space: nowrap;",
		],
		"description": "1行省略号"
	},
	"自定义-2行省略": {
		"prefix": "ellipsis2",
		"body": [
			"display: -webkit-box;",
			"overflow: hidden;",
			"text-overflow: ellipsis;",
			"-webkit-line-clamp: 2;",
			"-webkit-box-orient: vertical;"
		],
		"description": "2行省略号"
	},
	"自定义-flex居中": {
		"prefix": "flex-center",
		"body": [
			"display: flex;",
			"flex-direction: row;",
			"justify-content: center;",
			"align-items: center;",
		],
		"description": "水平垂直居中"
	},
}