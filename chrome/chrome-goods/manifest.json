{
  "manifest_version": 2,
  "name": "测试1",
  "description": "测试11111",
  "version": "2.0",
  "author" : "dengting",
  "permissions": [
      "tabs",
      "activeTab",
      "notifications",
      "storage"
  ],
  "icons": {
    "48" : "icon_48.png",
    "128" : "icon_128.png"
  },
  "background": {
    "persistent": true,
    "scripts": ["./js/back.js"]
  },
  "content_scripts":  [
    {
      "matches": ["http://testyapi.akulaku.com/*"],
      "js": ["./js/akulaku.js"],
      "run_at": "document_end",
      "comment":"接口生成"
    },
    {
      "matches": ["https://www.baidu.com/"],
      "js": ["./js/baidu.js"],
      "run_at": "document_end",
      "comment":"facebook信息获取"
    }
  ],
  "chrome_url_overrides": {
    "newtab": "./tab.html"
 },
  "web_accessible_resources": ["*.*","./js/*.*"]    
}
